{
  "manifest_version": 2,
  "name": "Wikipedia search",
  "author": "Drew",
  "description": "A wikipedia search tool",
  "homepage_url": "https://github.com/dmoneyballer/wikipedia_search",
  "version": "0.0.3",
  "icons": {
    "16": "images/Wikipedia-logo-16.png",
    "48": "images/Wikipedia-logo-48.png",
    "96": "images/Wikipedia-logo-96.png",
    "128": "images/Wikipedia-logo-128.png"
  },
  "browser_action": {
   "default_popup": "popup.html",
   "default_title": "Wikipedia Search",
   "default_icon": "images/Wikipedia-logo-128.png"
  },
  "permissions": [
    "webRequest",
    "https://en.wikipedia.org/*"
  ]
}
