{
  "manifest_version": 2,
  "name": "Wikipedia search",
  "author": "Drew",
  "description": "Template Browser Extension",
  "homepage_url": "https://github.com/dmoneyballer/wikipedia_search",
  "version": "0.0.1",
  "browser_action": {
